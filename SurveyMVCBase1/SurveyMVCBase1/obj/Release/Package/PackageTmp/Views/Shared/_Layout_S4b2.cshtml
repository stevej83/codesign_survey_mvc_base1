<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 英国企业家移民咨询测试</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    <div class="container">
        <header class="clearfix">
            <span>企业家移民咨询测试 </span>
            <h1>加入创业</h1>
        </header>
        <div class="main">
            @RenderBody()
        </div>
    </div>
    <footer><img src="~/Content/Site_Images/site_logo.png" alt="logo">Copyright © 2018 英国企业家移民咨询测试. All rights reserved</footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    <!-- for jquery datepicker, must put it after bootstrap-->
    <!-- jQuery UI CSS Reference -->
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.min.css")" rel="stylesheet" />
    <!-- Required jQuery and jQuery UI Javascript references -->
    <script src="@Url.Content("~/Scripts/jquery-3.1.1.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"></script>

    <!-- Show multiline editorfor / textareafor -->
    <script type="text/javascript">
        $(function () {
            var xS4bQ34 = $('#contS4bQ34AnsVar').val();
            var xS4bQ40 = $('#contS4bQ40AnsVar').val();
            var xS4bQ41 = $('#contS4bQ41AnsVar').val();
            var xS4bQ52 = $('#contS4bQ52AnsVar').val();
            var xS4bQ58 = $('#contS4bQ58AnsVar').val();

            $('#S4bQ34Answer').val(xS4bQ34.toString());
            $('#S4bQ40Answer').val(xS4bQ40.toString());
            $('#S4bQ41Answer').val(xS4bQ41.toString());
            $('#S4bQ52Answer').val(xS4bQ52.toString());
            $('#S4bQ58Answer').val(xS4bQ58.toString());
        })
    </script>
    <!-- end -->

    @Scripts.Render("~/bundles/jsdoc")
    @RenderSection("scripts", required: false)
</body>
</html>
