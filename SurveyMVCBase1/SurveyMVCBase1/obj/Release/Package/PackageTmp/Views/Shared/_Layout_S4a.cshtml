<!DOCTYPE html>
<html lang="en" class="no-js">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - 英国企业家移民咨询测试</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    <div class="container">
        <header class="clearfix">
            <span>企业家移民咨询测试 </span>
            <h1>独立创业</h1>
        </header>
        <div class="main">
            @RenderBody()
        </div>
    </div>
    <footer><img src="~/Content/Site_Images/site_logo.png" alt="logo">Copyright © 2018 英国企业家移民咨询测试. All rights reserved</footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")

    <!-- for jquery datepicker, must put it after bootstrap-->
    <!-- jQuery UI CSS Reference -->
    <link href="@Url.Content("~/Content/themes/base/jquery-ui.min.css")" rel="stylesheet" />
    <!-- Required jQuery and jQuery UI Javascript references -->
    <script src="@Url.Content("~/Scripts/jquery-3.1.1.min.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.12.1.min.js")"></script>

    <!-- Show multiline editorfor / textareafor -->
    <script type="text/javascript">
        $(function () {
            var xS4aQ1 = $('#contS4aQ1AnsVar').val();
            var xS4aQ2 = $('#contS4aQ2AnsVar').val();
            var xS4aQ3 = $('#contS4aQ3AnsVar').val();
            var xS4aQ7 = $('#contS4aQ7AnsVar').val();
            var xS4aQ8 = $('#contS4aQ8AnsVar').val();
            var xS4aQ9 = $('#contS4aQ9AnsVar').val();
            var xS4aQ10 = $('#contS4aQ10AnsVar').val();
            var xS4aQ13 = $('#contS4aQ13AnsVar').val();
            var xS4aQ14 = $('#contS4aQ14AnsVar').val();
            var xS4aQ15 = $('#contS4aQ15AnsVar').val();
            var xS4aQ33 = $('#contS4aQ33AnsVar').val();

            $('#S4aQ1Answer').val(xS4aQ1.toString());
            $('#S4aQ2Answer').val(xS4aQ2.toString());
            $('#S4aQ3Answer').val(xS4aQ3.toString());
            $('#S4aQ7Answer').val(xS4aQ7.toString());
            $('#S4aQ8Answer').val(xS4aQ8.toString());
            $('#S4aQ9Answer').val(xS4aQ9.toString());
            $('#S4aQ10Answer').val(xS4aQ10.toString());
            $('#S4aQ13Answer').val(xS4aQ13.toString());
            $('#S4aQ14Answer').val(xS4aQ14.toString());
            $('#S4aQ15Answer').val(xS4aQ15.toString());
            $('#S4aQ33Answer').val(xS4aQ33.toString());
        })
    </script>
    <!-- end -->

    @Scripts.Render("~/bundles/jsdoc")
    @RenderSection("scripts", required: false)
</body>
</html>
