            @Html.LabelFor(model => model.S4bQ60Answer, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @{
                    List<SelectListItem> S4bQ60AnswerListItems = new List<SelectListItem>();
                    S4bQ60AnswerListItems.Add(new SelectListItem
                    {
                        Text = "有",
